    "axios": "1.7.9",
    "axios-mock-adapter": "2.1.0",
    "chance": "1.1.12",
    "date-fns": "4.1.0",
    "lodash": "4.17.21",
    "pinia": "2.3.0",
    "remixicon": "4.6.0",
    "vee-validate": "4.15.0",
    "vite-plugin-vuetify": "2.0.4",
    "vue": "3.5.13",
    "vue-router": "4.5.0",
    "vue-tabler-icons": "2.21.0",
    "vue3-apexcharts": "1.8.0",
    "vue3-perfect-scrollbar": "2.0.0",
    "vue3-print-nb": "0.1.4",
