    "@vue/eslint-config-prettier": "10.1.0",
    "@vue/tsconfig": "0.7.0",
    "eslint": "9.17.0",
    "eslint-plugin-vue": "9.32.0",
    "gh-pages": "^6.3.0",
    "prettier": "3.4.2",
    "sass": "1.79.6",
    "sass-loader": "16.0.4",
    "typescript": "5.7.2",
    "typescript-eslint": "^8.19.0",
